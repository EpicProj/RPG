<?xml version="1.0" encoding="utf-8"?>
<data name ="MaterialMetaData" from="MaterialMetaData.xlsx" to = "MaterialMetaData.xml">
	<variable  name="AllMaterialList" type="list">
		<list name = "Material" sheetname="Material" mainKey = "MaterialID">
			<variable  name="MaterialID" col = "MaterialID" type="int"/>
			<variable  name="MaterialName" col = "MaterialName" type="string"/>
   		    <variable  name="NameEn" col = "NameEn" type="string"/>
			<variable  name="MaterialDesc" col = "MaterialDesc" type="string"/>
			<variable  name="Type" col = "Type" type="string"/>
			<variable  name="SubType" col = "SubType" type="string"/>
			<variable  name="BlockCapacity" col = "BlockCapacity" type="ushort"/>
			<variable  name="PriceBase" col = "PriceBase" type="int"/>
			<variable  name="MaterialIcon" col = "MaterialIcon" type="string"/>
			<variable  name="BG" col = "BG" type="string"/>
			<variable  name="UnitName" col = "UnitName" type="string"/>
			<variable  name="Rarity" col = "Rarity" type="string"/>
			<variable  name="ModelPath" col = "ModelPath" type="string"/>
		</list>
	</variable>

	<variable  name="AllMaterialTypeList" type="list">
		<list name = "MaterialType" sheetname="MaterialType" mainKey = "Type">
			<variable  name="Type" col = "Type" type="string"/>
			<variable  name="DefaultShow" col = "DefaultShow" type="bool"/>
			<variable  name="TypeName" col = "TypeName" type="string"/>
			<variable  name="TypeDesc" col = "TypeDesc" type="string"/>
			<variable  name="TypeIcon" col = "TypeIcon" type="string"/>
			<variable  name="SubTypeList" col = "SubTypeList" type="string"/>
		</list>
	</variable>

	<variable  name="AllMaterialSubTypeList" type="list">
		<list name = "MaterialSubType" sheetname="MaterialSubType" mainKey = "Type">
			<variable  name="SubType" col = "SubType" type="string"/>
			<variable  name="DefaultShow" col = "DefaultShow" type="bool"/>
			<variable  name="TypeName" col = "TypeName" type="string"/>
			<variable  name="TypeDesc" col = "TypeDesc" type="string"/>
			<variable  name="TypeIcon" col = "TypeIcon" type="string"/>
		</list>
	</variable>

</data>