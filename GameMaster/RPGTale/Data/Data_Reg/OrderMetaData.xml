<?xml version="1.0" encoding="utf-8"?>
<data name ="OrderMetaData" from="OrderMetaData.xlsx" to = "OrderMetaData.xml">
	<variable  name="AllOrderDataList" type="list">
		<list name = "OrderData" sheetname="OrderData" mainKey = "OrderID">
			<variable  name="OrderID" col = "OrderID" type="int"/>
			<variable  name="Name" col = "Name" type="string"/>
			<variable  name="Desc" col = "Desc" type="string"/>
			<variable  name="BGPath" col = "BGPath" type="string"/>
			<variable  name="Type" col = "Type" type="string"/>
			<variable  name="TimeLimit" col = "TimeLimit" type="float"/>
			<variable  name="OrderContent" col = "OrderContent" type="string"/>
			<variable  name="Reward" col = "Reward" type="int"/>
			<variable  name="Rarity" col = "Rarity" type="ushort"/>
			<variable  name="AppearConfig" col = "AppearConfig" type="string"/>
			<variable  name="DisAppearConfig" col = "DisAppearConfig" type="string"/>
		</list>
	</variable>

	<variable  name="AllOrderTypeDataList" type="list">
		<list name = "OrderTypeData" sheetname="OrderTypeData" mainKey = "TypeID">
			<variable  name="TypeID" col = "TypeID" type="string"/>
			<variable  name="Name" col = "Name" type="string"/>
			<variable  name="Desc" col = "Desc" type="string"/>
			<variable  name="IconPath" col = "IconPath" type="string"/>
		</list>
	</variable>

</data>