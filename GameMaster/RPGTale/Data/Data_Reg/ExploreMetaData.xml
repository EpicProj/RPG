		</list>
	</variable>

	<variable  name="AllExploreChooseList" type="list">
		<list name = "ExploreChoose" sheetname="ExploreChoose" mainKey = "ChooseID">
			<variable  name="ChooseID" col = "ChooseID" type="int"/>
