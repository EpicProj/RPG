			<variable  name="EnergyCost" col = "EnergyCost" type="ushort"/>
			<variable  name="EventID" col = "EventID" type="int"/>
		</list>
	</variable>

	<variable  name="AllExploreEventList" type="list">
		<list name = "ExploreEvent" sheetname="ExploreEvent" mainKey = "EventID">
			<variable  name="EventID" col = "EventID" type="int"/>
			<variable  name="Name" col = "Name" type="string"/>
			<variable  name="TitleName" col = "TitleName" type="string"/>
			<variable  name="Desc" col = "Desc" type="string"/>
			<variable  name="EventBG" col = "EventBG" type="string"/>
