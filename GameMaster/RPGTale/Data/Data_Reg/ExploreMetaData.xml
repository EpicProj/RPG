<?xml version="1.0" encoding="utf-8"?>
<data name ="ExploreMetaData" from="ExploreMetaData.xlsx" to = "ExploreMetaData.xml">
	<variable  name="AllExploreAreaList" type="list">
		<list name = "ExploreArea" sheetname="ExploreArea" mainKey = "AreaID">
			<variable  name="AreaID" col = "AreaID" type="int"/>
