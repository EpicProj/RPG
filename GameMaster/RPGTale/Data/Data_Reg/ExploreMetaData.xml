			<variable  name="NameTitle" col = "NameTitle" type="string"/>
			<variable  name="Desc" col = "Desc" type="string"/>
			<variable  name="IconPath" col = "IconPath" type="string"/>
			<variable  name="Unlock" col = "Unlock" type="bool"/>
			<variable  name="DefaultMissionCount" col = "DefaultMissionCount" type="ushort"/>
			<variable  name="ExploreList" col = "ExploreList" type="string"/>
		</list>
	</variable>
	
	<variable  name="AllExploreDataList" type="list">
		<list name = "ExploreData" sheetname="ExploreData" mainKey = "ExploreID">
			<variable  name="ExploreID" col = "ExploreID" type="int"/>
			<variable  name="MissionName" col = "MissionName" type="string"/>
			<variable  name="AreaName" col = "AreaName" type="string"/>
			<variable  name="MissionDesc" col = "MissionDesc" type="string"/>
			<variable  name="BGPath" col = "BGPath" type="string"/>
			<variable  name="CameraPos" col = "CameraPos" type="string"/>
			<variable  name="CameraRotation" col = "CameraRotation" type="string"/>
			<variable  name="TeamMaxNum" col = "TeamMaxNum" type="ushort"/>
			<variable  name="UnLockValue" col = "UnLockValue" type="ushort"/>
			<variable  name="RequirePreID" col = "RequirePreID" type="int"/>
			<variable  name="HardLevel" col = "HardLevel" type="ushort"/>
			<variable  name="Weight" col = "Weight" type="ushort"/>
			<variable  name="Depth" col = "Depth" type="ushort"/>
			<variable  name="SeriesID" col = "SeriesID" type="int"/>
		</list>
	</variable>

	<variable  name="AllExplorePointList" type="list">
		<list name = "ExplorePoint" sheetname="ExplorePoint" mainKey = "PointID">
			<variable  name="PointID" col = "PointID" type="int"/>
			<variable  name="SeriesID" col = "SeriesID" type="int"/>
