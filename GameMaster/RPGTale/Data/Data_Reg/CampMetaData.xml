		<list name = "CampData" sheetname="CampData" mainKey = "CampID">
			<variable  name="CampID" col = "CampID" type="int"/>
			<variable  name="CampName" col = "CampName" type="string"/>
			<variable  name="CampDesc" col = "CampDesc" type="string"/>
			<variable  name="IconCircle" col = "IconCircle" type="string"/>
			<variable  name="ValueMin" col = "ValueMin" type="float"/>
